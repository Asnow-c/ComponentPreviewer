{
    "compilerOptions": {
        "moduleResolution": "node",
        "jsx": "react",
        "jsxFactory": "h",
        "module": "ESNext",
        "target": "ESNext",
        "outDir": "../../out/res/preview",
        "lib": ["ESNext","DOM"],
        "sourceMap": false,
        "declaration": true,
        "rootDir": "src",
        "strict": true /* enable all strict type-checking options */,
        "skipLibCheck": true,
    },
    "exclude": ["node_modules"],
    "include": ["src"]
}
